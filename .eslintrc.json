{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@stylistic/js"
    ],
    "rules": {
        "@stylistic/js/indent": [
            "error",
            2
        ],
        "@typescript-eslint/no-confusing-void-expression": "off",
        "quotes": "off",
        "@typescript-eslint/quotes": "double"
    }
}